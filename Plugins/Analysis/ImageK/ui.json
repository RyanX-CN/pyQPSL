{
    "ImageKPluginUI": {
        "Type": "ImageKPluginUI",
        "backgroud_color": "#fafafa",
        "stretch": "10; 0",
        "widgets": [
            {
                "ObjectName": "view_image",
                "Type": "QPSLScalePixmapLabel",
                "aspect_ratio_mode": 0,
                "backgroud_color": "#fafafa"
            },
            {
                "Type": "QPSLVFrameList",
                "backgroud_color": "#b2b2b2",
                "margins": "10; 10; 10; 10",
                "spacing": 5,
                "widgets": [
                    {
                        "Type": "QPSLHFrameList",
                        "backgroud_color": "#7f7f7f",
                        "spacing": 25,
                        "stretch": "0; 5",
                        "v_size_policy": 4,
                        "widgets": [
                            {
                                "Type": "QPSLLabel",
                                "backgroud_color": "#333333",
                                "h_size_policy": 7,
                                "text": "image folder: ",
                                "v_size_policy": 4
                            },
                            {
                                "ObjectName": "btn_image_folder",
                                "Type": "QPSLPushButton",
                                "backgroud_color": "#bebebe",
                                "m_text": "choose",
                                "v_size_policy": 4
                            }
                        ]
                    },
                    {
                        "Type": "QPSLHFrameList",
                        "backgroud_color": "#e5e5e5",
                        "spacing": 12,
                        "stretch": "0; 5",
                        "v_size_policy": 4,
                        "widgets": [
                            {
                                "Type": "QPSLLabel",
                                "backgroud_color": "#333333",
                                "h_size_policy": 7,
                                "text": "camera folder: ",
                                "v_size_policy": 4
                            },
                            {
                                "ObjectName": "btn_camera_folder",
                                "Type": "QPSLPushButton",
                                "backgroud_color": "#b2b2b2",
                                "m_text": "choose",
                                "v_size_policy": 4
                            }
                        ]
                    },
                    {
                        "Type": "QPSLHFrameList",
                        "backgroud_color": "#999999",
                        "stretch": "0; 5",
                        "v_size_policy": 4,
                        "widgets": [
                            {
                                "Type": "QPSLLabel",
                                "backgroud_color": "#333333",
                                "h_size_policy": 7,
                                "text": "out folder name: ",
                                "v_size_policy": 4
                            },
                            {
                                "ObjectName": "line_out_name",
                                "Type": "QPSLLineEdit",
                                "backgroud_color": "#b2b2b2",
                                "v_size_policy": 4
                            }
                        ]
                    },
                    {
                        "Type": "QPSLHFrameList",
                        "backgroud_color": "#b2b2b2",
                        "h_size_policy": 4,
                        "spacing": 59,
                        "widgets": [
                            {
                                "Type": "QPSLLabel",
                                "backgroud_color": "#b2b2b2",
                                "text": "options:"
                            },
                            {
                                "Type": "QPSLHFrameList",
                                "backgroud_color": "#b2b2b2",
                                "widgets": [
                                    {
                                        "Type": "QPSLLabel",
                                        "backgroud_color": "#b2b2b2",
                                        "text": "down sample rate:"
                                    },
                                    {
                                        "ObjectName": "sbox_down_sample",
                                        "Type": "QPSLSpinBox",
                                        "backgroud_color": "#b2b2b2",
                                        "range": "1; 10000",
                                        "value": 1
                                    }
                                ]
                            },
                            {
                                "ObjectName": "cbox_flip_x",
                                "Type": "QPSLCheckBox",
                                "backgroud_color": "#191919",
                                "text": "flip x"
                            },
                            {
                                "ObjectName": "cbox_flip_y",
                                "Type": "QPSLCheckBox",
                                "backgroud_color": "#000000",
                                "text": "filp y"
                            }
                        ]
                    },
                    {
                        "ObjectName": "btn_start_convert",
                        "Type": "QPSLPushButton",
                        "m_text": "start"
                    }
                ]
            }
        ]
    }
}